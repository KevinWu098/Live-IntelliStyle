<div class="container-fluid container-fluid-custom pb-5 mb-5">
    <div class="row mb-4">
        <div class="col">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item">
                        <a href="/dashboard">Dashboard</a>
                    </li>
                    <li class="breadcrumb-item active">AI Add Clothing</li>
                </ol>
            </nav>
            <div class="col d-flex justify-content-between align-items-center">
                <h1 class="h3">AI Add Clothing</h1>
            </div>
        </div>
    </div>

    <form
        action="/dashboard/aiAdd"
        method="POST"
        class="position-relative"
        enctype="multipart/form-data"
    >
        <input
            type="file"
            class="form-control fw-bold border border-2 fs-4 mb-4"
            id="title"
            name="imageUpload"
            required
        />

        <div class="form-group">
            <button type="submit" class="btn btn-primary btn-lg">
                + Add Clothing
            </button>
        </div>
    </form>
</div>
